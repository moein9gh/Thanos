{
    "name": "clean-ts",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "rimraf ./dist && tsc -p tsconfig-build.json",
        "build:watch": "npm run build -- --watch",
        "dev": "nodemon -L --watch ./dist ./dist/index.js",
        "start:dev": "concurrently -kill-others-on-fail \"npm run build:watch\" \"npm run dev\"",
        "start": "node dist/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "^7.6.0",
        "nodemon": "^2.0.20",
        "rimraf": "^3.0.2",
        "typescript": "^4.9.3"
    },
    "_moduleAliases": {
        "@domain/*": "dist/domain"
    },
    "dependencies": {
        "@types/node": "^18.11.11"
    }
}
